<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.web.*?>

<fx:root type="javafx.scene.layout.VBox" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml" fx:controller="com.wordpress.erenha.arjuna.jauza.controller.BrowserController">
  <ToolBar>
    <items>
      <Button mnemonicParsing="false" onAction="#back" text="Back" />
      <Button mnemonicParsing="false" onAction="#forward" text="Forward" />
      <TextField fx:id="txt" onAction="#go" prefWidth="286.0" text="http://localhost/selectorgadget/" />
      <Button mnemonicParsing="false" onAction="#go" text="Go" />
      <ToggleButton mnemonicParsing="false" onAction="#inspect" text="Inspect" />
      <Button mnemonicParsing="false" onAction="#test" text="Test" visible="false" />
    </items>
  </ToolBar>
  <WebView fx:id="webx" minHeight="316.0" minWidth="959.0" onMouseClicked="#onMouseClick" prefHeight="445.0" prefWidth="1024.0" VBox.vgrow="ALWAYS">
    <VBox.margin>
      <Insets fx:id="x1" />
    </VBox.margin>
  </WebView>
  <HBox alignment="CENTER_LEFT" minHeight="14.0" prefHeight="20.0" prefWidth="1024.0" spacing="10.0" VBox.vgrow="NEVER">
    <children>
      <ProgressBar fx:id="progress" prefWidth="200.0" progress="0.0">
        <HBox.margin>
          <Insets left="10.0" />
        </HBox.margin>
      </ProgressBar>
      <Label fx:id="progressText" alignment="CENTER_RIGHT" contentDisplay="RIGHT" prefWidth="33.0" text="0%" textAlignment="RIGHT" />
      <Separator orientation="VERTICAL" prefHeight="200.0" />
      <Label fx:id="status" maxWidth="-Infinity" prefWidth="734.0" text="Ready" />
    </children>
  </HBox>
</fx:root>
